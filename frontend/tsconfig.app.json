{
    "compilerOptions": {
      "composite": true,
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react-jsx",

      "baseUrl": ".",
      "paths": {
      "@/*": [
        "./src/*"
      ]
    },
  }
}